package com.fif.ws.order.dto;

/**
 *
 * @author Administrator
 */
public class OrderFullMarketingSchemeContract {
    
    private String msCode  ;
    private String amount ;
    
    private ObjectPembiayaanSchemeObject schemeObject;

    public OrderFullMarketingSchemeContract() {
    }

    public OrderFullMarketingSchemeContract(String msCode, String amount, ObjectPembiayaanSchemeObject schemeObject) {
        this.msCode = msCode;
        this.amount = amount;
        this.schemeObject = schemeObject;
    }

    public String getMsCode() {
        return msCode;
    }

    public void setMsCode(String msCode) {
        this.msCode = msCode;
    }

    public String getAmount() {
        return amount;
    }

    public void setAmount(String amount) {
        this.amount = amount;
    }

    public ObjectPembiayaanSchemeObject getSchemeObject() {
        return schemeObject;
    }

    public void setSchemeObject(ObjectPembiayaanSchemeObject schemeObject) {
        this.schemeObject = schemeObject;
    }
    
}
